  "dependencies": {
    "colors": "^1.4.0",
    "concurrently": "^7.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "^8.30.0",
    "eslint-plugin-react": "^7.31.11",
    "express": "^4.18.2",
    "nodemon": "^2.0.20",
    "path": "^0.12.7"
  }
